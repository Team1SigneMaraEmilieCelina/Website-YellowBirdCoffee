---
// Layout.astro
import "../styles/generel.css";
const { title = "Yellow Bird Coffee" } = Astro.props;
---

<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1" />
  <link rel="icon" href="/img/icons/logo.ico" type="image/x-icon" />
  <title>{title}</title>
  <slot name="head" />
</head>

<style>
  html,
  body {
    margin: 0;
    width: 100%;
    height: 100%;
  }
</style>

<main>
  <slot />
</main>
